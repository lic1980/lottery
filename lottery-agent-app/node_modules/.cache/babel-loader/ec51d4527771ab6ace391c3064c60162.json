mn>\n                            <el-table-column prop=\"customer.tel\" label=\"投注人\">\n                            </el-table-column>\n                            <el-table-column prop=\"createTime\" label=\"投注时间\">\n                            </el-table-column>\n                            \n                        </el-table>\n                        <el-dialog\n                            title=\"投注\"\n                            name=\"file\"\n                            multiple=\"false\"\n                            :visible.sync=\"dialogVisible\"\n                            width=\"350px\"\n                            :before-close=\"handleClose\">\n                            <div>\n                                 <el-upload\n                                    class=\"avatar-uploader\"\n                                    accept=\"image/*\"\n                                    action=\"http://localhost:8080/api/v1/agents/1/bids/{bidId}/files\"\n                                    :show-file-list=\"false\"\n                                    :on-success=\"handleAvatarSuccess\"\n                                    :before-upload=\"beforeAvatarUpload\">\n                                    <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\">\n                                    <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\n                                </el-upload>\n                            </div>\n                        </el-dialog>\n                    </el-tab-pane>\n                    <el-tab-pane label=\"历史投注\" name=\"second\">\n                        <el-table :data=\"bids\" style=\"width: 100%\">\n                            <el-table-column  label=\"投注项\">\n                                <template slot-scope=\"scope\">  \n                                    <li v-for=\"(option, index) in scope.row.options\" v-bind:key=\"index\">\n                                        <span v-if=\"index == 0 || option.item.name != scope.row.options[index -1].item.name\">\n                                            {{ option.item.name }}\n                                        </span>\n                                        {{ option.text }}\n                                    </li>  \n                                </template>\n                            </el-table-column>\n                            <el-table-column prop=\"customer.tel\" label=\"投注人\">\n                            </el-table-column>\n                            <el-table-column prop=\"createTime\" label=\"投注时间\">\n                            </el-table-column>\n                            <el-table-column prop=\"bidTime\" label=\"完成时间\">\n                            </el-table-column>\n                        </el-table>\n                    </el-tab-pane>\n                </el-tabs>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n    data () {\n        return {\n            activeName: 'first',\n            agent: {},\n            dialogVisible: false,\n            imageUrl: \"\",\n            bids: []\n        };\n    },\n    methods: {\n      handleClick(tab, event) {\n        console.log(\"###########\");\n        console.log(tab, event);\n      },\n      handleBid(row) {\n          let data= {status: 'acked'};\n          axios\n            .patch('http://localhost:8080/api/v1/agents/1/bids/' + row.id, data, {headers: {'Content-Type': 'application/json'}})\n            .then(() => this.dialogVisible = true)\n            .catch(function (error) { \n                console.log(error);\n            });\n      },\n      handleClose(){\n          this.dialogVisible = false;\n      },\n      handleAvatarSuccess(res, file){\n          this.imageUrl = \"C:\\\\tmp\\\\lottery-image\\\\\" + res;\n      },\n      beforeAvatarUpload(){\n      }\n    },\n    mounted () {\n        axios\n            .get('http://localhost:8080/api/v1/agents/1')\n            .then(response => (this.agent = response.data))\n            .catch(function (error) { \n                console.log(error);\n            });\n        axios\n            .get('http://localhost:8080/api/v1/agents/1/bids?status=active&page=1&size=10')\n            .then(response => {\n                    this.bids = response.data.content;\n                })\n            .catch(function (error) { \n                console.log(error);\n            });\n    }\n}\n</script>\n\n<style>\n\n</style>"],"sourceRoot":"src/components"}]}